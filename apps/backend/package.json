{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "packageManager": "pnpm@10.0.0",
  "scripts": {
    "test": "vitest",
    "dev": "cross-env NODE_ENV=development ts-node-dev --exit-child --respawn src/main.ts",
    "start": "node dist/src/main.js",
    "build": "prisma generate && rm -rf ./dist && tsc && cp -r ./prisma/* ./dist/prisma",
    "typecheck": "tsc --noEmit",
    "codegen:gql": "graphql-codegen --config ./codegen.ts",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "db:pull": "prisma db pull",
    "codegen:prisma": "prisma generate",
    "codegen:microcms": "microcms_sdk_generator src/micro-cms-schemas src/micro-cms-schemas/generated.ts",
    "codegen": "pnpm run codegen:prisma && pnpm run codegen:gql",
    "ageage:scrape": "ts-node src/scripts/ageage-collector/index.ts"
  },
  "prisma": {
    "seed": "node dist/prisma/seed.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eddeee888/gcg-typescript-resolver-files": "^0.7.1",
    "@graphql-codegen/cli": "^5.0.0",
    "@types/node": "20.15.0",
    "@vitest/ui": "^1.2.2",
    "cross-env": "7.0.3",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0",
    "typescript": "5.4.5",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.2.2"
  },
  "dependencies": {
    "@graphql-tools/schema": "^10.0.2",
    "@prisma/client": "5.14.0",
    "cheerio": "1.0.0",
    "fastify": "^4.24.3",
    "graphql": "^16.8.1",
    "graphql-middleware": "^6.1.35",
    "graphql-scalars": "^1.22.4",
    "graphql-yoga": "5.0.0",
    "microcms_sdk_generator": "^0.1.1",
    "prisma": "5.14.0",
    "zod": "^3.22.4"
  }
}
